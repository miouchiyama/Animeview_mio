<h1>(自分の題名)</h1>
<h3>新規投稿ページ</h3>
<%= form_for @company do |f| %>

# データ型がstringで自由記述の入力フォーム
    <div class="field">
        <%= f.label :company_name %>
        <%= f.text_field :company_name, :size => 30 %>

    </div>
    <div class="field">
        <%= f.label :anime_name %>
        <%= f.text_field :anime_name, :size => 30 %>

    </div>

    <div class="field">
        <%= f.label :image %>
        <%= f.file_field :image %>
    </div>
    <h5>総合評価</h5>
        <div class="field">
            <%= f.label :overall  %>
            <%= f.number_field :overall, :size => 10 %>
    
        </div>


    <%= f.submit "投稿する" %>
<% end %>

<%= link_to "一覧に戻る", companies_path %>